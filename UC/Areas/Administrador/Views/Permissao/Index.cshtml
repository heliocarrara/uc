@model IEnumerable<UC.Models.Permissao>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ativo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.dataCriacao)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tipoPermissao)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ChaveAtivacao.cpf)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Usuario.cpf)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ativo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dataCriacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tipoPermissao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ChaveAtivacao.cpf)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Usuario.cpf)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.permissaoUID }) |
                @Html.ActionLink("Details", "Details", new { id = item.permissaoUID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.permissaoUID })
            </td>
        </tr>
    }

</table>
